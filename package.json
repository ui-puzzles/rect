{
  "name": "@ui-puzzles/rect",
  "version": "1.2.0",
  "private": false,
  "description": "An enterprise-class UI design language and React components implementation",
  "title": "Puzzle/rect",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "keywords": [
    "puzzle",
    "react",
    "component",
    "components",
    "ui",
    "design",
    "framework",
    "frontend",
    "react-component"
  ],
  "homepage": "https://github.com/ui-puzzles/rect",
  "bugs": {
    "url": "https://github.com/ui-puzzles/rect/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ui-puzzles/rect"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "react-transition-group": "^4.4.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run clean && npm run build:ts && npm run build:scss",
    "build:ts": "tsc -p tsconfig.prod.json",
    "build:scss": "sass ./src/styles/index.scss ./dist/index.css",
    "test": "react-scripts test --coverage",
    "test:stage": "cross-env CI=true react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s publish",
    "storybook-docs": "start-storybook --docs --no-manager-cache",
    "build-storybook": "npm run clean && build-storybook --docs -o docs",
    "clean": "rimraf ./dist ./docs",
    "lint:script": "eslint --ext js,ts,tsx src",
    "lint:style": "stylelint src/**/*.{scss,css} --fix",
    "lint": "npm run lint:script && npm run lint:style",
    "prepare": "husky install",
    "prepublishOnly": "npm run test:stage && npm run lint && npm run build",
    "release": "node scripts/release.js",
    "ui-cli": "node scripts/create-template-component.js"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npm run lint:script"
    ],
    "*.{scss,css}": [
      "npm run lint:style",
      "prettier --write --ignore-unknown"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/plugin-transform-react-jsx": "^7.16.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-docs": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.12",
    "@storybook/source-loader": "^6.3.12",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.171",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-transition-group": "^4.4.3",
    "@types/storybook__addon-info": "^5.2.4",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "babel-loader": "^8.2.3",
    "chalk": "^4.1.2",
    "commander": "^8.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.2",
    "lodash": "^4.17.21",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "replace-in-file": "^6.3.2",
    "rimraf": "^2.7.1",
    "sass": "^1.37.0",
    "shelljs": "^0.8.4",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  }
}
